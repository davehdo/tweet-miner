<%= form_with(model: tweet, local: true) do |form| %>
  <% if tweet.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(tweet.errors.count, "error") %> prohibited this tweet from being saved:</h2>

      <ul>
      <% tweet.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :keyword %>
    <%= form.text_field :keyword, id: :tweet_keyword %>
  </div>

  <div class="field">
    <%= form.label :unique_id %>
    <%= form.text_field :unique_id, id: :tweet_unique_id %>
  </div>

  <div class="field">
    <%= form.label :full_json %>
    <%= form.text_area :full_json, id: :tweet_full_json %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
